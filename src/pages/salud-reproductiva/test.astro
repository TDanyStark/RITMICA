---
import { URL_BASE } from "@/config";
import Layout from "@/layouts/Layout.astro";
import Footer from "@/components/Footer.astro";
import Header from "@/components/Header.astro";
import TestSVG from "@/assets/TestSVG.astro";
import SaludSVG from "@/assets/SaludSVG.astro";
import PautasSVG from "@/assets/Pautas.astro";
import HomeSVG from "@/assets/HomeSVG.astro";
---

<Layout>
  <section
    class="overflow-clip bg-right"
    style={`
      background-image: url('${URL_BASE}/img/bg_salud_reproductiva.webp');
      background-size: cover;
    `}
  >
    <div class="flex w-9xl max-w-full mx-auto flex-col min-h-screen">
      <Header with="tuciclo" />
      <main class="flex-1 p-4 flex flex-col md:flex-row gap-10 relative z-10">
        
      </main>
      <Footer />
    </div>
  </section>
</Layout>
